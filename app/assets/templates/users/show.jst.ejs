<h2><%= user.escape('first_name') %>'s Page</h2>
<p><em><%= user.escape('city') || "Somewhere" %>, <%= user.escape('state') || "Out there" %> &sdot; <%= age + " years old" || "Ageless"%></em></p>

<h2><a id="booksLink" href="#"><%= user.escape('first_name') %>'s Books</a></h2>
<div id="books"></div>

<h2><a id="clubsLink" href="#"><%= user.escape('first_name') %>'s Clubs</a></h2>
<div id="clubs"></div>

<% if(user.id === KR.currentUserID && KR.currentRole === 'parent') { %>
  <h2>My Kids</h2>
  <ul id="kids"></ul>
<% } %>